{%- from 'macros/helpers.html' import sprite, spriteTarget, importmapTarget -%}
<!doctype html>
<html lang="{{ meta.lang }}">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width,initial-scale=1,viewport-fit=cover">
  {% if images %}
    {# `images` can be collection populated by `cloudinary` task, or just set `images: true` in `data/global.mjs`
       to enable this if you use another way to upload images to cloudinary #}
    <meta http-equiv="delegate-ch"
          content="sec-ch-width https://res.cloudinary.com; sec-ch-dpr https://res.cloudinary.com; sec-ch-viewport-width https://res.cloudinary.com;">
  {% endif %}
  {% block head_pragma %}{% endblock %}
  <title>{% block title %}{{ meta.title }} {{ title }}{% endblock %}</title>
  {% if images %}
    <link rel="preconnect" href="https://res.cloudinary.com">
  {% endif %}
  {% block head_preconnect %}{% endblock %}
  <style>
    @font-face {
      font-family: "Innovator Grotesk";
      src: url("{{ "innovator-grotesk/variable.woff2" | assetUrl("fonts") }}") format("woff2");
      font-display: swap;
    }
    @font-face {
      font-family: "PP Stellar";
      font-weight: 700;
      src: url("{{ "pp-stellar/bold.latin.woff2" | assetUrl("fonts") }}") format("woff2");
      font-display: swap;
      unicode-range: U+0-FF,U+131,U+152,U+153,U+2BB,U+2BC,U+2C6,U+2DA,U+2DC,U+2000-206F,U+2074,U+20AC,U+2122,U+2191,U+2193,U+2212,U+2215,U+FEFF,U+FFFD;
    }
    @font-face {
      font-family: "PP Stellar";
      font-weight: 700;
      src: url("{{ "pp-stellar/bold.latin2.woff2" | assetUrl("fonts") }}") format("woff2");
      font-display: swap;
      unicode-range: U+0100-0101,U+0104-0130,U+0132-0151,U+0154-017F,U+018F,U+0192,U+01A0-01A1,U+01AF-01B0,U+01FA-01FF,U+0218-021B,U+0237,U+0259,U+1E80-1E85,U+1E9E,U+20A1,U+20A4,U+20A6,U+20A8-20AA,U+20AD-20AE,U+20B1-20B2,U+20B4-20B5,U+20B8-20BA,U+20BD,U+20BF
    }
    @font-face {
      font-family: "PP Stellar";
      font-weight: 300;
      src: url("{{ "pp-stellar/light.latin.woff2" | assetUrl("fonts") }}") format("woff2");
      font-display: swap;
      unicode-range: U+0-FF,U+131,U+152,U+153,U+2BB,U+2BC,U+2C6,U+2DA,U+2DC,U+2000-206F,U+2074,U+20AC,U+2122,U+2191,U+2193,U+2212,U+2215,U+FEFF,U+FFFD;
    }
    @font-face {
      font-family: "PP Stellar";
      font-weight: 300;
      src: url("{{ "pp-stellar/light.latin2.woff2" | assetUrl("fonts") }}") format("woff2");
      font-display: swap;
      unicode-range: U+0100-0101,U+0104-0130,U+0132-0151,U+0154-017F,U+018F,U+0192,U+01A0-01A1,U+01AF-01B0,U+01FA-01FF,U+0218-021B,U+0237,U+0259,U+1E80-1E85,U+1E9E,U+20A1,U+20A4,U+20A6,U+20A8-20AA,U+20AD-20AE,U+20B1-20B2,U+20B4-20B5,U+20B8-20BA,U+20BD,U+20BF
    }
  </style>
  <link rel="stylesheet" href="{{ 'index.css' | assetUrl('stylesheets') }}">
  {% block head_stylesheets %}{% endblock %}
  <link rel="preload" as="font" crossorigin="anonymous"
        href="{{ "innovator-grotesk/variable.woff2" | assetUrl("fonts") }}">
  {% block head_preload %}{% endblock %}
  {% block head_async_scripts %}{% endblock %}
  <meta name="description" content="{{ description or meta.description }}">
  <meta name="generator" content="{{ generator or '@hckr_/blendid - static site generator and assets pipeline' }}">
  <meta name="color-scheme" content="light dark">
  <link rel="manifest" href="{{ 'manifest.json' | assetUrl('static') }}">
  <link rel="canonical" href="{{ canonicalUrl or meta.url }}">
  {% include "shared/app-icons.html" %}
  {% block head %}{% endblock %}
</head>
